{
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.sneak": true,
  "vim.surround": true,

  // ===============================NORMAL MODE ===============================
  "vim.normalModeKeyBindings": [
    // {
    //   "before": ["w"],
    //   "after": ["e"]
    // },
    {
      "before": ["<leader>", "k"],
      "commands": ["editor.action.jumpToBracket"]
    },
    {
      "before": ["<leader>", "j"],
      "commands": ["editor.action.jumpToBracket"]
    },
    {
      "before": ["leader", "d"],
      "commands": ["editor.action.jumpToBracket"]
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["workbench.action.gotoLine"]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["g", "g"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["<S-C>"],
      "commands": [
        "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
      ]
    },
    {
      "before": ["<leader>", "f"],
      "commands": [
        "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
      ]
    },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.fontZoomIn"]
    },

    {
      "before": ["<C-j>"],
      "commands": ["editor.action.fontZoomOut"]
    },
    {
      "before": ["<S-f>"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    },
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    },

    {
      "before": ["f"],
      "after": ["*"]
    },
    {
      "before": ["c", "c"],
      "after": ["c", "i", "w"]
    },
    {
      "before": ["v", "v"],
      "after": ["<S-v>"]
    },
    {
      "before": ["b"],
      "commands": ["cursorWordEndLeft"]
    },

    {
      "before": ["enter"],
      "commands": ["editor.action.revealDefinition"]
    },

    {
      "before": ["<leader>", "t"],
      "commands": ["editor.fold"]
    },
    {
      "before": ["<leader>", "r"],
      "commands": ["editor.unfold"]
    },
    {
      "before": ["<C-m>"],
      "commands": [":marks"]
    },
    {
      "before": ["<tab>"],
      "commands": [":q"]
    },
    {
      "before": ["`"],
      "commands": [":q"]
    },

    {
      "before": ["<tab>"],
      "commands": [":q"]
    },

    {
      "before": ["x"],
      "commands": ["deleteRight"]
    },
    {
      "before": ["<leader>", "q"],
      "commands": [":q"]
    },
    // {
    //   "before": ["<tab>"],
    //   "commands": ["workbench.action.nextEditor"]
    // },
    {
      "before": ["<S-tab>"],
      "commands": ["workbench.action.previousEditor"]
    }
  ],
  // ===============================INSERT MODE ===============================
  "vim.insertModeKeyBindings": [
    // {
    //   "before": ["<leader>", "f"],
    //   "commands": ["workbench.action.showAllEditors"]
    // },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.fontZoomIn"]
    },

    {
      "before": ["<C-j>"],
      "commands": ["editor.action.fontZoomOut"]
    },
    {
      "before": ["<C-h>"],
      "commands": ["workbench.view.explorer"]
    },
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    },

    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],

  // ===============================VISUAL MODE ===============================
  "vim.visualModeKeyBindings": [
    {
      "before": ["f"],
      "commands": ["actions.find"]
    },
    // {
    //   "before": ["<leader>", "f"],
    //   "commands": ["actions.find"]
    // },
    {
      "before": ["<S-p>"],
      "commands": ["editor.action.copyLinesDownAction"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["editor.action.fontZoomIn"]
    },

    {
      "before": ["<C-j>"],
      "commands": ["editor.action.fontZoomOut"]
    },
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    }, // moveLineDown
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    }, // moveLineUp
    // {
    //   "before": ["<tab>"],
    //   "commands": [":q"]
    // },
    {
      "before": ["v", "v"],
      "after": ["<S-v>"]
    },
    {
      "before": [">"],
      "after": [">", "g", "v"]
    },
    {
      "before": ["<"],
      "after": ["<", "g", "v"]
    }
  ],
  // ===============================NON RECURSIVE MODE ===============================
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "/"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<leader>", "w"],
      "commands": [":w"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    // {
    //   "before": ["<leader>", "f"],
    //   "commands": ["editor.action.rename"]
    // },
    {
      "before": ["<leader>", "h"],
      "after": ["_"]
    },
    {
      "before": ["<leader>", "l"],
      "after": ["$"]
    }
  ],
  "vim.leader": "<space>",

  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    // VS Code new marker @ next occurence
    "<C-c>": false,
